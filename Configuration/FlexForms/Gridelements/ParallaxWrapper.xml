<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<general>
			<ROOT>
				<type>array</type>
				<TCEforms>
					<sheetTitle>LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:flexforms.general</sheetTitle>
				</TCEforms>
				<el>
					<!-- parallax speed factor -->
					<speedFactor>
						<TCEforms>
							<label>Speed Factor</label>
							<config>
								<type>select</type>
								<renderType>selectSingle</renderType>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">0.1</numIndex>
										<numIndex index="1">0.1</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">0.2</numIndex>
										<numIndex index="1">0.2</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">0.3</numIndex>
										<numIndex index="1">0.3</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">0.4</numIndex>
										<numIndex index="1">0.4</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">0.5</numIndex>
										<numIndex index="1">0.5</numIndex>
									</numIndex>
									<numIndex index="5" type="array">
										<numIndex index="0">0.6</numIndex>
										<numIndex index="1">0.6</numIndex>
									</numIndex>
									<numIndex index="6" type="array">
										<numIndex index="0">0.7</numIndex>
										<numIndex index="1">0.7</numIndex>
									</numIndex>
									<numIndex index="7" type="array">
										<numIndex index="0">0.8</numIndex>
										<numIndex index="1">0.8</numIndex>
									</numIndex>
									<numIndex index="8" type="array">
										<numIndex index="0">0.9</numIndex>
										<numIndex index="1">0.9</numIndex>
									</numIndex>
								</items>
								<default>0.3</default>
							</config>
						</TCEforms>
					</speedFactor>

					<!-- background color -->
					<bgColor>
						<TCEforms>
							<label>Background color</label>
							<config>
								<type>input</type>
								<renderType>colorpicker</renderType>
								<checkbox></checkbox>
								<eval>trim</eval>
								<size>10</size>
							</config>
						</TCEforms>
					</bgColor>
					<!-- padding Top&Bottom -->
					<paddingTopBottom>
						<TCEforms>
							<label>Padding (top and bottom in rem)</label>
							<config>
								<type>input</type>
								<checkbox></checkbox>
								<eval>int</eval>
								<size>6</size>
								<default></default>
							</config>
						</TCEforms>
					</paddingTopBottom>
				</el>
			</ROOT>
		</general>
	</sheets>
</T3DataStructure>